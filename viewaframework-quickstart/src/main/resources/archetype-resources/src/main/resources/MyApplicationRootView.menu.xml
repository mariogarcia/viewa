<!-- *********************************************************************************** -->
<!-- *********************** MENUBAR AND TOOLBAR DEFINITION **************************** -->
<!-- *********************************************************************************** -->
<!-- * In this file you can define the menu-bar and the tool-bar of the view. Define   * -->
<!-- * your own structure and hierarchy. Add icons and keystrokes to your items.       * -->
<!-- *********************************************************************************** -->
<menuBar>
	<root text="root">
		<exit icon="org/viewaframework/widget/icon/fan/img/door/door_out.png" keyStroke="CTRL_T" />
	</root>
	<edit text="edit">
		<copy icon="org/viewaframework/widget/icon/fan/img/page/page_copy.png" keyStroke="CTRL_C" text="copy" />
		<cut icon="org/viewaframework/widget/icon/fan/img/cut/cut.png" keyStroke="CTRL_X" text="cut" />
		<paste icon="org/viewaframework/widget/icon/fan/img/paste/paste_plain.png" keyStroke="CTRL_V" text="paste" />
		<separator text="editSeparator" />
		<selectAll keyStroke="CTRL_A" text="selectAll" />
	</edit>
	<help text="help">
		<about icon="org/viewaframework/widget/icon/fan/img/misc/help.png" keyStroke="CTRL_H" text="about"/>
	</help>
</menuBar>